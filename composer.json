{
    "name": "lava/magmacore",
    "description": "A simple PHP framework for building web applications",
    "keywords": [
        "framework", "php", "magmacore"
    ],
    "type": "library",
    "license": "MIT",
    "readme": "",
    "time": "",
    "support":{
        "issues": "https://github.com/DeborahFWatson21/MagmaCore/issues",
        "source": "https://github.com/DeborahFWatson21/MagmaCore",
        "wiki": "https://github.com/DeborahFWatson21/MagmaCore/wiki"
    },
    "authors": [
        {
            "name": "D F Watson",
            "email": "deborah.fwatson21@gmail.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "alpha",
    "require": {
        "php": "7.4.2|^8.0",
        "ext-mbstring": "*",
        "ext-iconv": "*",
        "ext-pdo": "*",
        "ext-json":"*",
        "ext-curl":"*",
        "ext-simplexml":"*"
    },
    "autoload": {
        "psr-4": {
            "Magma\\": "src/Magma"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Magma\\Tests\\": "tests/"
        }
    }
}
